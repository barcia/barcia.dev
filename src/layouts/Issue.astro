---
import Layout from "./Layout.astro";

const { issue } = Astro.props

const parsedDate = new Date(issue.slug);
const { month, year } = {
	month: parsedDate.toLocaleString('default', { month: 'long' }),
	year: parsedDate.getFullYear(),
};
---
<Layout title={'aaa'}>
  	<h1>{month} of {year}</h1>
</Layout>